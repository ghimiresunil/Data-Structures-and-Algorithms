   "source": [
    "word = input()\n",
    "dct = {}\n",
    "for i in word:\n",
    "    dct[i] = dct.get(i, 0) + 1\n",
    "\n",
    "dct = sorted(dct.items(), key=lambda x: (-x[1], x[0]))\n",
    "for i in dct:\n",
    "    print(i[0],'->', i[1])"
